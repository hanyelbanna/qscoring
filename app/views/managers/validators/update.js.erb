$("ul.errors").html("")
<% if @validator.errors.any? %>
  <% @validator.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
  	$("#modal_place").replaceWith("<%= j(render 'layouts/ajax_error_modal', messages: @validator.errors) %>");
    $('#myModal').modal()
<% else %>
$("#data_<%= @validator.id %>").replaceWith("<%= j(render @validator) %>");
$('#data-form').slideUp(350);
<% end %>