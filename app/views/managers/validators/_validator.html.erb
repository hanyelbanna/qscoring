<tr id="data_<%= validator.id %>">
  <td><%= validator.first_name %></td>
  <td><%= validator.last_name %></td>
  <td><%= link_to edit_managers_validator_path(validator), remote: true do %>
		<button class="btn btn-primary btn-xs"><i class="fa fa-edit"></i> Edit</button>
    <% end %>
  </td>
  <td><%= link_to validator, remote: true, method: :delete, data: { confirm: 'Are you sure?' } do %>
  	<button class="btn btn-danger btn-xs"><i class="fa fa-trash-o"></i> Delete</button>
    <% end %>
  </td>
</tr>