$("ul.errors").html("")
<% if @frequency.errors.any? %>
  <% @frequency.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
  	$("#modal_place").replaceWith("<%= j(render 'layouts/ajax_error_modal', messages: @frequency.errors) %>");
    $('#myModal').modal()
<% else %>
$("#data_<%= @frequency.id %>").replaceWith("<%= j(render @frequency) %>");
$('#data-form').slideUp(350);
<% end %>