<div class="page-head">
  <div class="page-title">
    <h1>users <small>All Users</small></h1>
  </div>
  <div class="page-toolbar">
    <%= link_to new_managers_user_path, remote: true do %>
    <button class="btn blue"><i class="fa fa-plus-circle"></i> New User</button>
    <% end %>
  </div>
</div>
<%= render partial: "layouts/breadcrumb" %>
<%= render partial: "layouts/portlet_header", locals: {header_color: "red", header_rows: "8", header_title: "Edit Users"} %>
<div class="table-responsive">
<%= render partial: "layouts/pagination", locals: {pages_collection: @users} %>
<%= render partial: "layouts/search", locals: {this_page: "managers_users"} %>



          <table class="table table-striped table-bordered table-hover">
          <thead>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Edit</th>
            <th>Delete</th>
          </tr>
          </thead>
          <tbody id="data-table">
            <%= render @users %>
          </tbody>
          </table>

<%= render partial: "layouts/pagination", locals: {pages_collection: @users} %>
<%= render partial: "layouts/portlet_form", locals: {header_color: "purple", header_title: "User Form"} %>
